    If ReadUIConfig("刷活动") Then 
        模式 = 2
        //sp3?
        //
        dim sp= ReadUIConfig("sp")
        TracePrint "sp =",sp
        Select Case sp
        Case 1
        Case 2
        Case 3
        End Select
    End If
    If ReadUIConfig("炼金术") Then 
        模式=3
    End If
    If ReadUIConfig("刷好感") Then 
        模式=4
    End If
    If ReadUIConfig("每日挑战") Then 
        模式=5
    End If
End Function
